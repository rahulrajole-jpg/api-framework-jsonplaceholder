{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["id", "name", "username", "email"],
  "properties": {
    "id": {"type": "integer"},
    "name": {"type": "string"},
    "username": {"type": "string"},
    "email": {"type": "string", "format": "email"},
    "address": {"type": "object"},
    "phone": {"type": "string"},
    "website": {"type": "string"},
    "company": {"type": "object"}
  }
}
